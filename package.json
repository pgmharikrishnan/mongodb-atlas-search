{
  "name": "@pgmharikrishnan/mongodb-atlas-search",
  "version": "1.0.2r2",
  "description": "Automatic mapping generation and index management for atlas search based on mongoose schema",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsnd --respawn src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepublish": "tsc"
  },
  "author": "Harikrishnan K",
  "license": "ISC",
  "dependencies": {
    "mongoose": "^5.12.3",
    "urllib": "^2.37.1"
  },
  "devDependencies": {
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "keywords": [
    "mongodb",
    "mongodb atlas",
    "atlas search",
    "index mapping",
    "atlas api",
    "mongoose schema",
    "automatic mapping from mongoose schema",
    "mongodb-atlas-search",
    "mongodb atlas search"
  ]
}
